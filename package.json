{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Email feedback system back-end",
  "main": "index.js",
  "engines": {
    "node": "8.1.2",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node back-end/index.js",
    "server": "nodemon back-end/index.js",
    "client": "npm run start --prefix front-end",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix front-end && npm run build --prefix front-end"
  },
  "author": "Devashish G Patel",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "mongoose": "^5.0.16",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "redux-thunk": "^2.2.0",
    "stripe": "^5.8.0"
  }
}
